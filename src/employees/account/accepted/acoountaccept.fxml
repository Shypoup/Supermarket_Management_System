      </JFXButton>
      <JFXButton contentDisplay="RIGHT" layoutX="490.0" layoutY="14.0" onAction="#loadBack" prefHeight="31.0" prefWidth="87.0" styleClass="back" stylesheets="@acoountaccept.css" text=" رجوع">
